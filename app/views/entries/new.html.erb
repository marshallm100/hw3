<h1>New Entry</h1>

<form action="/places/<%= @place.id %>/entries" method="post">
  <p>
    <label for="title">Title</label>
    <input type="text" name="title">
  </p>
   <p>
    <label for="description">Description</label>
    <input type="text" name="description">
  </p>
   <p>
    <label for="occurred_on">Occured_on</label>
    <input type="date" name="occurred_on">
  </p>
    <p>
       <label for="place_id">Place_id</label> <input type="text" name="place_id" value="<%= @place["id"] %>"> 
       </p>
  </p>
  <button>Submit</button>
</form>